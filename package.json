{
    "name": "brewthers",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build-firebase": "vue-cli-service build  && firebase deploy --only hosting",
        "firebase-set": "firebase functions:config:get > functions/.runtimeconfig.json"
    },
    "dependencies": {
        "@quasar/extras": "^1.0.0",
        "axios": "^0.21.1",
        "emailjs-com": "^2.4.1",
        "firebase": "^7.22.0",
        "moment": "^2.27.0",
        "quasar": "^1.0.0",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vue-slick-carousel": "^1.0.6",
        "vue2-google-maps": "^0.10.7",
        "vue2-smooth-scroll": "^1.3.0",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-service": "^4.4.1",
        "babel-plugin-transform-imports": "1.5.0",
        "node-sass": "^4.13.0",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-quasar": "~2.0.2",
        "vue-template-compiler": "^2.6.10"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "babel": {
        "plugins": [
            [
                "transform-imports",
                {
                    "quasar": {
                        "transform": "quasar/dist/babel-transforms/imports.js",
                        "preventFullImport": true
                    }
                }
            ]
        ]
    }
}
